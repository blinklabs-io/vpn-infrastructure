# Add configuration for the AWS Load Balancer Controller
replicaCount: 1

image:
  repository: public.ecr.aws/eks/aws-load-balancer-controller
  tag: v2.13.0

clusterName: {{ .Values.eks_lb.clusterName }}

serviceAccount:
  annotations:
    eks.amazonaws.com/role-arn: {{ .Values.eks_lb.serviceAccount.roleArn }}
